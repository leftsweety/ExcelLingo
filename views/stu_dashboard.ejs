  <section class="section-sm bg-primary">
                  <div class="container">
      <div class="row">    
          <div class="col-md-4 col-sm-6 mb-4 mb-md-0">
                  <div class="text-center">
                      <h2 class="text-white"><%=myCourse.length%></h2>
                      <h5 class="text-white"> Courses</h5>
                  </div>
      
              </div>
          <div class="col-md-4 col-sm-6 mb-4 mb-md-0">
              <div class="text-center">
                  <h2 class="text-white"><%=numOfCplt%></h2>
                  <h5 class="text-white">Completed</h5>
              </div>
  
          </div>
          <div class="col-md-4 col-sm-6 mb-4 mb-md-0">
              <div class="text-center">
                  <h2 class="text-white"> <%=numOfWait%></h2>
                  <h5 class="text-white">Waiting for grade</h5>
              </div>
  
          </div>
      </div>
  </div>
  </section>
  
  <section class="section">
  
      <div class="container">
      <!-- course list -->
      <h2 class="section-title">My Courses</h2>
      <div class="row justify-content-center">
      
    <!-- course item -->
          <% for(var i=0; i< myCourse.length; i++){%>
          <div class="col-lg-4 col-sm-6 mb-5">
              <div class="card p-0 border-primary rounded-0 hover-shadow">
                
              <img class="card-img-top rounded-0" src="images/courses/img-<%=myCourse[i].course_id%>.jpeg" alt="course thumb">
              <div class="card-body">
                  <!-- <ul class="list-inline mb-2">
                  <li class="list-inline-item"><i class="ti-calendar mr-1 text-color"></i>02-14-2018</li>
                  <li class="list-inline-item"><a class="text-color" href="#">Humanities</a></li>
                  </ul> -->
                  <a href="course-single.html">
                  <h4 class="card-title"><%=myCourse[i].course_name%></h4>
                  </a>
                  <p class="card-text mb-4"> <%=myCourse[i].description%></p>
                  <div class="d-md-table-cell text-right pr-0 pr-md-4"><a href="/recording?course_id=<%=myCourse[i].course_id%>" class="btn btn-primary">view</a> </div>
                  <div class="d-md-table-cell text-right pr-0 pr-md-4"><a href="/rateTutor/<%=myCourse[i].course_id%>" class="btn btn-primary">Rate Tutor</a> </div>
              </div>
              </div>
          </div>
          <% } %>
      </div>
      <h2 class="section-title">Recommendations</h2>
      <div class="row justify-content-center">
    
  <!-- course item -->
          <% for(var i=0; i< RecommendCourse.length; i++){%>
              <div class="col-lg-4 col-sm-6 mb-5">
                  <div class="card p-0 border-primary rounded-0 hover-shadow">
                      <img class="card-img-top rounded-0" src="images/courses/img-<%=RecommendCourse[i].course_id%>.jpeg" alt="course thumb">
                      <div class="card-body">
                      <!-- <ul class="list-inline mb-2">
                          <li class="list-inline-item"><i class="ti-calendar mr-1 text-color"></i>02-14-2018</li>
                          <li class="list-inline-item"><a class="text-color" href="#">Humanities</a></li>
                      </ul> -->
                      <a href="course-single.html">
                          <h4 class="card-title"><%=RecommendCourse[i].course_name%></h4>
                      </a>
                      <p class="card-text mb-4"> <%=RecommendCourse[i].description%></p>
                      <div class="d-md-table-cell text-right pr-0 pr-md-4"><a href="/course_info?course_id=<%=RecommendCourse[i].course_id%>" class="btn btn-primary">details</a> </div>
                      <div class="d-md-table-cell text-right pr-0 pr-md-4"><h3>$ <%=RecommendCourse[i].price%></h3> </div>
                      </div>
                  </div>
          </div>
          <% } %>
      </div>
  </section>
  
  
