<main>
<section class="page-title-section overlay" data-background="images/backgrounds/page-title.jpg">
    <div class="container">
        <div class="row">
        <div class="col-md-8">
            <ul class="list-inline custom-breadcrumb">
            <li class="list-inline-item"><a class="h2 text-primary font-secondary">Recording</a></li>
            <li class="list-inline-item text-white h3 font-secondary nasted"><%= courseInfo.course_descriotion%></li>
            </ul>
            <p class="text-lighten">Watch class'video and try to practice! Your recording will be viewed by tutor</p>
        </div>
        </div>
    </div>
    </section>

 <section class="section">
        <div class="container">
          <div class="row">
                <div class="col-md-3 mb-5 mb-md-0">
                        <h4 class="mb-4">Class lists:</h4>
                        <ul class="list-styled">
                                <% for ( var i = 0 ; i < class_info.class_id.length ; i++){ %>
                            <a href="/recording?class_id=<%=class_info.class_id[i]%>&course_id=<%=courseInfo.course_id%>">
                            <li class="mb-3" class="text-color"> <%= class_info.class_description[i] %></li>
                        </a>
                            <!-- <a href="/recording?class_id=<%=class_info.class_id[i]%>&course_id=<%=courseInfo.course_id%>" ><%= class_info.class_description[i] %> </a> <br> -->
                            <% } %>
                        </ul>
                    </div>
            <div class="col-md-4 mb-5">
                <a href= <%= curClassInfo.curClass_link%> target="_blank">
                    
              <img class="img-fluid w-100" src="images/video/play1.png"   alt="course thumbr">
              </a>
            </div>
            <div class="col-md-4 mb-5">
              <h3><%= courseInfo.course_name%></h3>
              <p class="mb-5"><%= courseInfo.course_descriotion%></p>
              <h4 class="card-title">Tutor's feedback</h4>
              <text class="form-control mb-3" style="height:150px;width:525px; overflow:scroll;"  ><%= recording_info.comment%></text>
              
              <br>
              <a href="https://drive.google.com/drive/my-drive" target="_blank">
                    <h4 class="card-title">Record</h4>
                  </a>
                <form name="input" action="./save_recording" method="get">
                    <input type="text"  class="form-control mb-3" value= "<%=recording_info.recording_link%>" name="link">
                    <input type="hidden" name="class_id" value= <%= curClassInfo.curClass_id %> /> 
                    <input type="hidden" name="tutor_id" value= <%= courseInfo.tutor_id %> />         
                    <input type="submit" class="btn btn-primary" value="Submit">
                </form>

            </div>
        </div>
</section>

